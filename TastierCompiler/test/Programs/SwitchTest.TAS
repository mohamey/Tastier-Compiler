program SwitchTest{
	void Main(){
		int temp[10];
		temp[7] := 14;
		switch(temp[7]){
			case 10:
				temp[2]:= 100;
			case 11: 
				temp[2] := 200;
			case 12:
				temp[2] := 300;
			case 13:
				temp[2] := 400;
			case 140:
				temp[2] := 500;
			case 15:
				temp[2] := 600;
			default:
				temp[2] := 1000;
		}
		write temp[2];
	}
}
